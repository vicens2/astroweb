{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/entry.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/_astro/(.*)",
      "dest": "/_astro/$1"
    },
    {
      "src": "/(.*)\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot)",
      "dest": "/$1.$2"
    }
  ]
}
