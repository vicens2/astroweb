{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/entry.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_astro/.*",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/_astro/"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server/entry.mjs"
    }
  ]
}
